function SWEP:PrimaryAttack()
  local Owner = self:GetOwner()

  local pl = Owner:GetEyeTrace().Entity
  if not pl:IsPlayer then return end
  if not IsValid(pl) then return end

  Owner:ConCommand("ulx ban", pl)
end
